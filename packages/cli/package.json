{
  "name": "@leanmcp/cli",
  "version": "0.4.2",
  "description": "Command-line interface for scaffolding LeanMCP projects",
  "bin": {
    "leanmcp": "bin/leanmcp.js"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "bin",
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "dev": "tsup src/index.ts --format esm --dts --watch",
    "test": "echo \"No tests yet\" && exit 0",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.0.1",
    "@vitejs/plugin-react": "^4.3.0",
    "archiver": "^7.0.1",
    "autoprefixer": "^10.4.16",
    "chalk": "^5.3.0",
    "chokidar": "^4.0.0",
    "commander": "^12.0.0",
    "fs-extra": "^11.2.0",
    "glob": "^11.0.0",
    "ora": "^8.1.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "vite": "^5.4.0",
    "vite-plugin-singlefile": "^2.3.0"
  },
  "devDependencies": {
    "@types/archiver": "^6.0.3",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^25.0.2",
    "rimraf": "^6.1.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LeanMCP/leanmcp-sdk.git",
    "directory": "packages/cli"
  },
  "homepage": "https://github.com/LeanMCP/leanmcp-sdk#readme",
  "bugs": {
    "url": "https://github.com/LeanMCP/leanmcp-sdk/issues"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "cli",
    "scaffolding",
    "generator",
    "typescript"
  ],
  "author": "LeanMCP <admin@leanmcp.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}